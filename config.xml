<?xml version='1.0' encoding='utf-8'?>
<widget id="delfyn.rng" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RNGimg</name>
    <description>
        Generate random IMGur images
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Delfyn
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/mipmap-ldpi/ic_launcher.png" />
        <icon density="mdpi" src="res/icon/mipmap-mdpi/ic_launcher.png" />
        <icon density="hdpi" src="res/icon/mipmap-hdpi/ic_launcher.png" />
        <icon density="xhdpi" src="res/icon/mipmap-xhdpi/ic_launcher.png" />
        <icon density="xxhdpi" src="res/icon/mipmap-xxhdpi/ic_launcher.png" />
        <icon density="xxxhdpi" src="res/icon/mipmap-xxxhdpi/ic_launcher.png" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <engine name="android" spec="~6.2.3" />
    <plugin name="com-sarriaroman-photoviewer" spec="~1.1.10" />
</widget>
